{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.4.2/"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react",
    "jsxImportSourceTypes": "npm:@types/react"
  },
  "nodeModulesDir": "auto",
  "fmt": {
    "exclude": [
      "_site",
      "_cache",
      "node_modules",
      "_broken_links.json"
    ]
  }
}
